{
    "apps": [
        {
            "auto-detach": "false",
            "cmd": "gamescope -W 1920 -H 1080 -f -- steam -applaunch 1903340",
            "elevated": "false",
            "exclude-global-prep-cmd": "false",
            "exit-timeout": "5",
            "image-path": "",
            "name": "Clair Obscur",
            "output": "",
            "wait-all": "true"
        },
        {
            "image-path": "desktop.png",
            "name": "Desktop"
        },
        {
            "image-path": "desktop.png",
            "name": "Low Res Desktop",
            "prep-cmd": [
                {
                    "do": "xrandr --output HDMI-1 --mode 1920x1080",
                    "undo": "xrandr --output HDMI-1 --mode 1920x1200"
                }
            ]
        },
        {
            "auto-detach": "false",
            "cmd": "MoonDeckBuddy --exec MoonDeckStream",
            "elevated": "false",
            "exclude-global-prep-cmd": "false",
            "exit-timeout": "5",
            "image-path": "",
            "name": "MoonDeckStream",
            "output": "",
            "prep-cmd": [
                {
                    "do": "kscreen-doctor output.DP-1.mode.1920x1080@60",
                    "undo": "kscreen-doctor output.DP-1.mode.2560x1440@75"
                }
            ],
            "wait-all": "true"
        },
        {
            "auto-detach": true,
            "detached": [
                "setsid steam steam://open/bigpicture"
            ],
            "exclude-global-prep-cmd": false,
            "exit-timeout": 5,
            "image-path": "steam.png",
            "name": "Steam Big Picture",
            "prep-cmd": [
                {
                    "do": "kscreen-doctor output.DP-1.mode.1920x1080@60",
                    "undo": "kscreen-doctor output.DP-1.mode.2560x1440@75"
                }
            ],
            "wait-all": true
        }
    ],
    "env": {
        "PATH": "$(PATH):$(HOME)/.local/bin"
    }
}